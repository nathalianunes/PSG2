package model;

import javax.persistence.Entity;

@Entity
public class Ponto {
	private int id;
	private String nome;
	private double tempo;
	
	public Ponto(String nome, double tempo) {
		super();
		this.nome = nome;
		this.tempo = tempo;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public double getTempo() {
		return tempo;
	}

	public void setTempo(double tempo) {
		this.tempo = tempo;
	}
	
}
