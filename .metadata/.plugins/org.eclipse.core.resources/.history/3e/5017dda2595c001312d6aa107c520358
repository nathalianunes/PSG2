package model;

import javax.persistence.Entity;

@Entity
public class Restricoes {
	private int id;
	private Ponto ponto;
	
	public Restricoes(String ponto) {
		super();
		this.ponto = ponto;
	}

	public String getPonto() {
		return ponto;
	}

	public void setPonto(String ponto) {
		this.ponto = ponto;
	}
	
}
